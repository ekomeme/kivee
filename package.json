{
  "name": "kivee",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:staging": "vite --host --mode staging",
    "dev:production": "vite --host --mode production",
    "build": "tsc -b && vite build",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:production": "tsc -b && vite build --mode production",
    "deploy:staging": "npm run build:staging && firebase use default && firebase deploy --only hosting:staging",
    "deploy:production": "npm run build:production && firebase use default && firebase deploy --only hosting:prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "preview:production": "vite preview --mode production"
  },
  "dependencies": {
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "googleapis": "^169.0.0",
    "i18n-nationality": "^1.4.0",
    "libphonenumber-js": "^1.12.33",
    "lucide-react": "^0.553.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-phone-number-input": "^3.4.14",
    "react-router-dom": "^7.9.5",
    "react-select": "^5.10.2",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
